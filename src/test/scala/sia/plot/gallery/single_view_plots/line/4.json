{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "data": null,
  "mark": {
    "type": "line",
    "tooltip": true
  },
  "encoding": {
    "x": {"field": "date", "type": "temporal"},
    "y": {"field": "price", "type": "quantitative"},
    "color": {
      "field": "symbol",
      "type": "nominal",
      "scale": {
        "scheme": "set2"
      },
      "legend": {
        "type": "symbol",
        "symbolType": "stroke"
      }
    }
  }

}