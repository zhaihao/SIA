{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "data": null,
  "mark": {
    "type": "line",
    "tooltip": true,
    "point": true
  },
  "encoding": {
    "x": {
      "timeUnit": "year",
      "field": "date",
      "type": "temporal"
    },
    "y": {
      "aggregate": "mean",
      "field": "price",
      "type": "quantitative"
    },
    "color": {
      "field": "symbol",
      "type": "nominal",
      "legend": {
        "type": "symbol",
        "symbolType": "circle"
      },
      "scale": {
        "scheme": "set2"
      }
    }
  }
}